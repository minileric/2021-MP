import numpy as np

#так как log возвращает натуральный логарифм, нужно пользоваться функцией перехода
x = 8
(np.log(x) / np.log(2)) #логарифм x по основанию два

e=2.7
x=1
A=28
c=np.log(((e**(np.cos(x))**A)*np.tan(x**2))/((A/9)*x +(x**2/3)))/(np.log(2*x))
print (c)
x=100
c=np.log(((e**(np.cos(x))**A)*np.tan(x**2))/((A/9)*x +(x**2/3)))/(np.log(2*x))
print (c)
